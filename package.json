  "license": "ISC",
  "bugs": {},
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^15.0.0",
    "express":"*",
    "body-parser":"*",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.9"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7"
  }
